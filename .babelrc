{
  "presets": ["next/babel", "@emotion/babel-preset-css-prop"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "root": "./",
          "actions": "./actions",
          "components": "./components",
          "data": "./data",
          "lang": "./lang",
          "modules": "./modules",
          "pages": "./pages",
          "reducers": "./reducers",
          "sagas": "./sagas",
          "selectors": "./selectors",
          "static": "./static",
          "styles": "./styles",
          "utils": "./utils"
        }
      }
    ]
  ],
}
